<html lang="es">

  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="icon" href="build/images/logo-login-white.png" />
    <title>The Warrior Baber Shop</title>
    <!-- Jquery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    {{!
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.5.0/dist/js/bootstrap.min.js"></script> }}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <!-- Bootstrap -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" />
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <!-- Materialize -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <!-- Iconos -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />
    <script src="https://kit.fontawesome.com/f6414e1f4b.js" crossorigin="anonymous"></script>
    <!-- SweetAlert -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    {{! Calendario }}
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/fullcalendar@5.10.0/main.css" />
    <script src="https://cdn.jsdelivr.net/npm/fullcalendar@5.10.0/main.js"></script>
    {{! Estilos }}
    <link rel="stylesheet" href="build/css/registro.css" />
    <title>Login</title>
  </head>

  <body>
    <div id="contentLogo">
      <img src="./build/images/img.png" id="imgLog1" />
      <img src="./build/images/logo-login-white.png" id="imgLog2" />
    </div>
    <form class="container formulario">
      <div class="form">
        <a onclick="cancelar()" class="back"><i class="fa-solid fa-circle-arrow-left"></i></a>
        <i class="fa-solid fa-circle-user"></i>
        <h3 class="center">Registro</h3>
        <div class="d-flex justify-content-center">
          <div class="input-field me-3">
            <i class="prefix fa-solid fa-user" id="nomUsuIco" name="exclamation"></i>
            <input type="text" id="nomUsu" data-length="15" />
            <i class="prefix fa-solid fa-exclamation exclamationSign" id="nomUsuExc" name="exclamation"></i>
            <label for="nomUsu">Nombre de usuario</label>
          </div>
          <div class="input-field ms-3">
            <i class="prefix fa-solid fa-envelope" id="emailIco" name="exclamation"></i>
            <input type="email" id="email" data-length="40" placeholder="ejemplo@gmail.com" />
            <i class="prefix fa-solid fa-exclamation exclamationSign" id="emailExc" name="exclamation"></i>
            <label for="email">Correo</label>
          </div>
        </div>
        <div class="d-flex justify-content-center">
          <div class="input-field me-3">
            <i class="prefix fa-solid fa-id-card" id="nombreIco" name="exclamation"></i>
            <input type="text" id="nombre" data-length="15" />
            <i class="prefix fa-solid fa-exclamation exclamationSign" id="nombreExc" name="exclamation"></i>
            <label for="nombre">Nombre</label>
          </div>
          <div class="input-field ms-3">
            <i class="prefix fa-solid fa-id-card" id="apellidosIco" name="exclamation"></i>
            <input type="text" id="apellidos" data-length="20" />
            <i class="prefix fa-solid fa-exclamation exclamationSign" id="apellidosExc" name="exclamation"></i>
            <label for="apellidos">Apellidos</label>
          </div>
        </div>
        <div class="d-flex justify-content-center">
          <div class="input-field me-3">
            <i class="prefix fa-solid fa-phone" id="celularIco" name="exclamation"></i>
            <input type="text" id="celular" oninput="validarCelu('celular')" data-length="15" placeholder="(123) 456-7890" />
            <i class="prefix fa-solid fa-exclamation exclamationSign" id="celularExc" name="exclamation"></i>
            <label for="celular">Celular</label>
          </div>
          <div class="input-field ms-3">
            <i class="prefix fa-solid fa-lock" id="passIco" name="exclamation"></i>
            <input type="password" id="pass" data-length="25" placeholder="●●●●●●●●" />
            <i class="prefix fa-solid fa-exclamation exclamationSign" id="passExc" name="exclamation"></i>
            <label for="pass">Contraseña</label>
          </div>
        </div>
        <div class="contReg ingresar">
          <div class="ing center" onclick="registrar()"><a class="aReg">Registrarme</a></div>
        </div>
      </div>
    </form>
    <script src="build/js/registro.js"></script>
  </body>

</html>