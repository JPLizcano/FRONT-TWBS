<html lang="es">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="icon" href="build/images/logo-login-white.png" />
    <title>The Warrior Baber Shop</title>
    <!-- Jquery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    {{!
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.5.0/dist/js/bootstrap.min.js"></script> }}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <!-- Bootstrap -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" />
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <!-- Materialize -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <!-- Iconos -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />
    <script src="https://kit.fontawesome.com/f6414e1f4b.js" crossorigin="anonymous"></script>
    <!-- SweetAlert -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    {{! Calendario }}
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/fullcalendar@5.10.0/main.css" />
    <script src="https://cdn.jsdelivr.net/npm/fullcalendar@5.10.0/main.js"></script>
    {{! Estilos }}
  <link rel="stylesheet" href="build/css/registro.css" />
  <title>Login</title>
</head>

<body>
  <div>
    <img src="./build/images/img.png" id="imgLog1">
    <img src="./build/images/logo-login-white.png" id="imgLog2">
  </div>
  <form class="container formulario">
    <h3 class="center">Registro</h3>
    <div class="d-flex justify-content-center">
      <div class="input-field me-3">
        <i class="prefix fa-solid fa-user"></i>
        <input type="text" id="nomUsu" />
        <label for="nomUsu">Nombre de usuario</label>
      </div>
      <div class="input-field ms-3">
        <i class="material-icons prefix">email</i>
        <input type="email" id="email" class="" />
        <label for="email">Correo</label>
        <span class="helper-text">ejemplo@mail.com</span>
      </div>
    </div>
    <div class="d-flex justify-content-center">
      <div class="input-field me-3">
        <i class="prefix fa-solid fa-id-card"></i>
        <input type="text" id="nombre" class="" />
        <label for="nombre">Nombre</label>
      </div>
      <div class="input-field ms-3">
        <i class="prefix fa-solid fa-id-card"></i>
        <input type="text" id="apellidos" />
        <label for="apellidos">Apellidos</label>
      </div>
    </div>
    <div class="d-flex justify-content-center">
      <div class="input-field me-3">
        <i class="prefix fa-solid fa-phone"></i>
        <input type="text" id="celular" class="" />
        <label for="celular">Celular</label>
      </div>
      <div class="input-field ms-3">
        <i class="material-icons prefix">lock</i>
        <input type="password" id="pass" />
        <label for="pass">Contraseña</label>
        <span class="helper-text">La contraseña debe tener entre 8-15
          caracteres de la "a" a la "z" y números.</span>
      </div>
    </div>
    <div class="center botones">
      <button type="button" class="waves-effect waves-perso btn white" onclick="ingresar()">Registrarse</button>
      <button type="button" class="waves-effect waves-perso btn white" onclick="cancelar()">Cancelar</button>
    </div>
  </form>
  <script src="build/js/registro.js"></script>
</body>

</html>