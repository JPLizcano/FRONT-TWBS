<html lang="en">

<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <link rel="icon" href="build/images/barber.jpg" />
  <title>The Warrior Baber Shop</title>
  <!-- Jquery -->
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
  <!-- Bootstrap -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" />
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
  <!-- Materialize -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css" />
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
  <!-- Iconos -->
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />
  <script src="https://kit.fontawesome.com/f6414e1f4b.js" crossorigin="anonymous"></script>
  <!-- SweetAlert -->
  <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
  <link href="./build/css/estilos.css" rel="stylesheet" />
  <link href="./build/css/insumos.css" rel="stylesheet" />

</head>

<body>
  {{>header}}
  <main class="content">
    <div>
      <h1 class="d-flex justify-content-center tit">Lista De Insumos</h1>
    </div>
    <div>
      <div class="acciones">
        <a class="btnsConfi" id="btnAgg"><i class="fa fa-user-plus"></i></a>
      </div>
      <table class="table table-striped table-hover">
        <thead>
          <tr>
            <th>Cantidad</th>
            <th>Nombre del insumo</th>
            <th>Descripción</th>
            <th>Stock máximo</th>
            <th>Stock mínimo</th>
            <th>Fecha de compra</th>
            <th>Precio Unitario</th>
            <th>Valor total</th>
            <th>Valor total</th>
            <th>Actualizar información</th>
          </tr>
        </thead>
        <tbody id="tablaInsu">
        </tbody>
      </table>
    </div>
  </main>
  {{>footer}}
  <script src="./build/js/funciones.js"></script>
</body>

</html>